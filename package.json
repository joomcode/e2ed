{
  "name": "e2ed",
  "version": "0.0.19",
  "description": "E2E utils",
  "keywords": [
    "E2E",
    "testcafe"
  ],
  "author": "uid11",
  "license": "MIT",
  "bin": {
    "e2ed-docker": "./bin/run-tests-in-docker.sh",
    "e2ed-local": "./bin/run-tests-local.sh"
  },
  "bugs": "https://github.com/uid11/e2ed/issues",
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://github.com/uid11/e2ed#readme",
  "repository": "uid11/e2ed",
  "type": "module",
  "dependencies": {
    "testcafe": "1.15.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "7.30.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "prettier": "2.3.2",
    "typescript": "4.4.0-beta"
  },
  "scripts": {
    "copy:meta": "cp -r bin LICENSE package.json README.md .testcaferc.json build",
    "lint": "npm run lint:es & npm run lint:prettier & npm run lint:types & wait",
    "lint:es": "eslint --ext=.js,.ts .",
    "lint:prettier": "prettier --check --ignore-path=.gitignore '**/*.{yaml,json,ts,js,md}'",
    "lint:types": "tsc --noEmit",
    "prebuild": "rm -rf build",
    "build": "tsc; echo 'Build done.'",
    "postbuild": "npm run copy:meta",
    "prepublish": "build",
    "publish": "npm publish build"
  }
}
